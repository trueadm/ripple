{
  "name": "ripple",
  "description": "Ripple is an elegant TypeScript UI framework",
  "license": "MIT",
  "author": "Dominic Gannaway",
  "version": "0.2.185",
  "type": "module",
  "module": "src/runtime/index-client.js",
  "main": "src/runtime/index-client.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ripple-TS/ripple.git",
    "directory": "packages/ripple"
  },
  "bugs": {
    "url": "https://github.com/Ripple-TS/ripple/issues"
  },
  "homepage": "https://ripplejs.com",
  "keywords": [
    "ripple",
    "UI",
    "framework"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "worker": "./src/runtime/index-server.js",
      "browser": "./src/runtime/index-client.js",
      "default": "./src/runtime/index-server.js"
    },
    "./package.json": "./package.json",
    "./server": {
      "types": "./types/server.d.ts",
      "default": "./src/server/index.js"
    },
    "./compiler": {
      "types": "./src/compiler/index.d.ts",
      "require": "./src/compiler/index.js",
      "default": "./src/compiler/index.js"
    },
    "./validator": {
      "types": "./types/index.d.ts",
      "require": "./validator/index.js",
      "default": "./src/validator/index.js"
    },
    "./internal/client": {
      "default": "./src/runtime/internal/client/index.js"
    },
    "./internal/server": {
      "default": "./src/runtime/internal/server/index.js"
    },
    "./jsx-runtime": {
      "types": "./src/jsx-runtime.d.ts",
      "import": "./src/jsx-runtime.js",
      "default": "./src/jsx-runtime.js"
    }
  },
  "imports": {
    "#client": "./src/runtime/internal/client/types.d.ts",
    "#client/constants": "./src/internal/client/constants.js",
    "#server": "./src/runtime/internal/server/types.d.ts",
    "#compiler": "./src/compiler/types/index.d.ts",
    "#parser": "./src/compiler/types/parse.d.ts",
    "#public": "./types/index.d.ts"
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "catalog:default",
    "@sveltejs/acorn-typescript": "catalog:default",
    "acorn": "catalog:default",
    "clsx": "catalog:default",
    "devalue": "catalog:default",
    "esm-env": "catalog:default",
    "esrap": "catalog:default",
    "is-reference": "catalog:default",
    "magic-string": "catalog:default",
    "muggle-string": "catalog:default",
    "zimmerframe": "catalog:default"
  },
  "devDependencies": {
    "@types/estree": "catalog:default",
    "@types/estree-jsx": "catalog:default",
    "@types/node": "catalog:default",
    "@typescript-eslint/types": "catalog:default",
    "typescript": "catalog:default",
    "@volar/language-core": "catalog:default",
    "vscode-languageserver-types": "catalog:default"
  },
  "peerDependencies": {
    "ripple": "workspace:*"
  }
}
