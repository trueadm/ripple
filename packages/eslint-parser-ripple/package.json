{
  "name": "eslint-parser-ripple",
  "version": "0.2.112",
  "description": "ESLint parser for Ripple (.ripple files)",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trueadm/ripple.git",
    "directory": "packages/eslint-parser-ripple"
  },
  "bugs": {
    "url": "https://github.com/trueadm/ripple/issues"
  },
  "homepage": "https://ripplejs.com",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "parser",
    "ripple"
  ],
  "author": "Dominic Gannaway",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "ripple": "*"
  },
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-typescript": "^1.4.13"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.6",
    "@types/node": "^24.3.0",
    "ripple": "workspace:*",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  }
