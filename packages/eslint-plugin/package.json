{
  "name": "@ripple-ts/eslint-plugin",
  "version": "0.2.185",
  "description": "ESLint plugin for Ripple",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ripple"
  ],
  "author": "Dominic Gannaway",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ripple-TS/ripple.git",
    "directory": "packages/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/Ripple-TS/ripple/issues"
  },
  "homepage": "https://ripplejs.com",
  "scripts": {
    "dist": "perl -pi -e 's/\"main\": \"src\\/index.ts\"/\"main\": \"dist\\/index.js\"/' package.json",
    "src": "perl -pi -e 's/\"main\": \"dist\\/index.js\"/\"main\": \"src\\/index.ts\"/' package.json",
    "build": "pnpm dist && tsdown && pnpm src || pnpm src",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "eslint": "catalog:peer",
    "@typescript-eslint/parser": "catalog:peer",
    "@ripple-ts/eslint-parser": "workspace:*"
  },
  "devDependencies": {
    "@types/eslint": "catalog:default",
    "@types/estree": "catalog:default",
    "@types/node": "catalog:default",
    "@typescript-eslint/parser": "catalog:default",
    "eslint": "catalog:default",
    "@ripple-ts/eslint-parser": "workspace:*",
    "tsdown": "catalog:default",
    "typescript": "catalog:default",
    "vitest": "catalog:default"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
