import { array } from 'ripple';

export component App() {
	let items = array(1, 2, 3);

	<button onClick={() => items[items.$length] = items.$length + 1}>{'increment'}</button>

	<Child items={items} />
}

component Child({ items }) {
	<pre>{JSON.stringify(items)}</pre>
	<pre>{items.$length}</pre>
}