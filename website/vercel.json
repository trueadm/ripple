{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"cleanUrls": true,
	"buildCommand": "vitepress build",
	"outputDirectory": ".vitepress/dist",
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin-when-cross-origin"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				}
			]
		},
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/github",
			"destination": "https://github.com/Ripple-TS/ripple",
			"permanent": false
		},
		{
			"source": "/docs",
			"destination": "/docs/introduction",
			"permanent": false
		}
	]
}
